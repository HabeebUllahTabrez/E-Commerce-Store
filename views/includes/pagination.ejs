<section class="pagination">
    <% if(currPage != 1) { %>
        <a href="?page=1">1</a>
    <% } %>
    <% if(hasPreviousPage) { %>
        <a href="?page=<%= previousPage %>"><<</a>
    <% } %>
    <a href="?page=<%= currPage %>" class="active"><%= currPage %></a>
    <% if(hasNextPage) { %>
        <a href="?page=<%= nextPage %>">>></a>
    <% } %>
    <% if(currPage !== lastPage) { %>
        <a href="?page=<%= nextPage %>"><%= lastPage %></a>
    <% } %>
</section>